{
  "name": "express-ws",
  "version": "1.2.0",
  "description": "tdd with express",
  "main": "app.js",
  "scripts": {
    "start:dev": "cross-env NODE_ENV=development npm run migrate && cross-env NODE_ENV=development nodemon index.js",
    "start": "cross-env NODE_ENV=production npm run migrate && cross-env NODE_ENV=production node index.js",
    "test": "cross-env NODE_ENV=test jest tests --watch && cross-env NODE_ENV=test npm run clean-up",
    "test:staging": "cross-env NODE_ENV=staging npm run migrate && cross-env NODE_ENV=staging jest --runInBand --detectOpenHandles --forceExit && cross-env NODE_ENV=staging npm run clean-up",
    "clean-up": "node test-cleanUp.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "migrate": "sequelize-cli db:migrate"
  },
  "author": "drKreshel",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "config": "^3.3.3",
    "express": "^4.17.1",
    "express-validator": "^6.9.2",
    "file-type": "^16.2.0",
    "i18next": "^19.8.4",
    "i18next-fs-backend": "^1.0.7",
    "i18next-http-middleware": "^3.0.6",
    "nodemailer": "^6.4.17",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "sqlite3": "^5.0.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.8.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-wesbos": "^1.0.1",
    "eslint-plugin-html": "^6.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "jest": "^26.6.3",
    "jest-watch-typeahead": "^0.6.1",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.1",
    "smtp-server": "^3.8.0",
    "supertest": "^6.0.1"
  },
  "prettier": {
    "printWidth": 120
  },
  "jest": {
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules",
      "<rootDir>/config"
    ]
  }
}
